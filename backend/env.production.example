# Database
DATABASE_URL="mysql://root@localhost:3306/lagiah_prod"

# NextAuth
NEXTAUTH_SECRET="your-super-secret-production-key-change-this"
NEXTAUTH_URL="https://api.lagiah.com"

# API Configuration
WHOIS_API_URL="https://api.whois.com"
WHOIS_API_KEY="your-whois-api-key"

# RDAP Configuration (Primary API)
USE_RDAP="true"
USE_RDASH="false"

# RDASH Configuration (Fallback)
RDASH_API_URL="https://api.rdash.id/v1"
RDASH_RESELLER_ID="your-reseller-id"
RDASH_API_KEY="your-api-key"

# Redis (for caching and sessions)
REDIS_URL="redis://localhost:6379"

# WhatsApp API (if using)
WHATSAPP_API_URL="https://api.whatsapp.com"
WHATSAPP_API_KEY="your-whatsapp-api-key"

# Security
JWT_SECRET="your-jwt-secret-key-change-this"
ENCRYPTION_KEY="your-32-character-encryption-key"

# Logging
LOG_LEVEL="info"
NODE_ENV="production"

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# CORS
ALLOWED_ORIGINS="https://lagiah.com,https://www.lagiah.com" 